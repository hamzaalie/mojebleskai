{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "header",
          "key": "x-vercel-404-page"
        }
      ],
      "destination": "/",
      "permanent": false
    }
  ]
}
